  "scripts": {
    "clean": "rimraf ./public",
    "prebuild": "npm install && npm run clean",
    "start": "cross-env NODE_ENV=dev nodemon server/app.js",
    "start:prod": "cross-env NODE_ENV=prod node server/app.js",
    "start:stage": "cross-env NODE_ENV=stage node server/app.js",
    "start:dev": "cross-env NODE_ENV=dev node server/app.js",
    "start:test": "cross-env NODE_ENV=test node server/app.js",
    "start:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build:dev": "npm run prebuild && cross-env NODE_ENV=dev webpack --config webpack.config.production.js",
    "build:test": "npm run prebuild && cross-env NODE_ENV=test webpack --config webpack.config.production.js",
    "build:stage": "npm run prebuild && cross-env NODE_ENV=stage webpack --config webpack.config.production.js",
    "build:prod": "npm run prebuild && cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "eslint": "eslint src --fix --quiet --ext .js,.jsx",
    "analyz-report": "webpack --profile --json > stats.json",
    "analyz-unused": "webpack --json | webpack-unused ./src"
  },
  "repository": {
    "type": "git",
